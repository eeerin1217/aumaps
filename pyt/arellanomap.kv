            size_hint: .70, .30
            pos_hint: {"center_x": 0.5, "center_y": 0.5}
            background_color: (0.06, .36, .4, .675) 
            font_size: 25
            on_release: 
                app.root.current = "florentino"
                root.manager.transition.direction = "left"

        Button: 
            text: "Paulino Cayco Hall(PCH)"
            size_hint: .70, .30
            pos_hint: {"center_x": 0.5, "center_y": 0.5}
            background_color: (0.06, .36, .4, .675) 
            font_size: 25
            on_release: 
                app.root.current = "paulino"
                root.manager.transition.direction = "left"

        Button: 
            text: "Plaridel Hall(PH)"
            size_hint: .70, .30
            pos_hint: {"center_x": 0.5, "center_y": 0.5}
            background_color: (0.06, .36, .4, .675) 
            font_size: 25
            on_release: 
                app.root.current = "plaridel"
                root.manager.transition.direction = "left"

        Button: 
            text: "Annex Building(AB)"
            size_hint: .70, .30
            pos_hint: {"center_x": 0.5, "center_y": 0.5}
            background_color: (0.06, .36, .4, .675) 
            font_size: 25
            on_release: 
                app.root.current = "annex"
                root.manager.transition.direction = "left"

        Button: 
            text: "Lebron Building(LB)"
            size_hint: .70, .30
            pos_hint: {"center_x": 0.5, "center_y": 0.5}
            background_color: (0.06, .36, .4, .675) 
            font_size: 25
            on_release: 
                app.root.current = "lebron"
                root.manager.transition.direction = "left"

        Button: 
            text: "Athletic Building(AB)"
            size_hint: .70, .30
            pos_hint: {"center_x": 0.5, "center_y": 0.5}
            background_color: (0.06, .36, .4, .675) 
            font_size: 25
