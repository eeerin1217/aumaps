            background_color: (0.06, .36, .4, .675) 
            font_size: 25
            on_release: 
                app.root.current = "esb"
                root.manager.transition.direction = "left"

        Button:
            text: "Go back to Main Screen"
            size_hint: .70, .30
            pos_hint: {"center_x": 0.5, "center_y": 0.5}
            background_color: (0.06, .36, .4, .675) 
            font_size: 25
            on_release: 
                app.root.current = "main"
                root.manager.transition.direction = "down"

        Label: 
            text:"Arellano Tanga"
            size_hint: .70, .10
            pos_hint: {"center_x": 0.5, "center_y": 0.5}

<RHScreen>:
    name:"rizal"

    canvas:
        Color: 
            rgba: (0.1529, 0.6157, 0.9608, 0.8) 
        Rectangle: 
            source: 'back.jfif'
            size: root.width, root.height 
            pos: self.pos

    ActionBar:
        size: root.width, 50
        pos: 0, 650

        ActionView:
            ActionPrevious:
                title: "Floors"
                with_previous: False
                app_icon: 'C:\\Users\\Axl\\Desktop\\pyt\\images\\arellanologo.png'

            ActionButton:
                text:"1st"

            ActionButton:
                text:"2nd" 

            ActionButton:
                text:"3rd" 

            ActionButton:
                text:"4th"  
    
    BoxLayout:
        orientation: 'vertical'
        Label:
            text: 'RHScreen'

        Button:
            text: 'Go back to Main'
            size_hint: .5, None
            pos_hint: {"center_x": .5}
            on_release: 
                app.root.current = "select"
                root.manager.transition.direction = "right"

