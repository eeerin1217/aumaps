WindowManager:
    MainWindow:
    SelectScreen:
    RHScreen:
    FCHScreen:
    PCHScreen:
    PHScreen:
    ABScreen:
    LBScreen:
    ABBScreen:
    ESBScreen:

<MainWindow>:
    name: "main"

    canvas:
        Color: 
            rgba: (0.1529, 0.6157, 0.9608, 0.8)
        Rectangle: 
            source: 'back.jfif'
            size: root.width, root.height 
            pos: self.pos
    
    BoxLayout:
        orientation: 'vertical'
        spacing: 10
        padding: 20

        Label:
            text: "Arellano University Juan Sumulong Campus Map"
            size_hint: .70, .10
            pos_hint: {"center_x": 0.5, "center_y": 0.5}
        
        Image:
            source: 'C:\\Users\\Axl\\Desktop\\pyt\\images\\gatearellano.png'
            fit_mode: "cover"
            size_hint: (None, None)
            size: ("400dp", "300dp")
            pos_hint: {"center_x": 0.5, "center_y": 0.5}

        Button:
            text: "Check Buildings"
            size_hint: .70, .30
            pos_hint: {"center_x": 0.5, "center_y": 0.5}
            background_color: (0.06, .36, .4, .675) 
            font_size: 25
            on_release: 
                app.root.current = "select"
                root.manager.transition.direction = "up"


<SelectScreen>:
    name: "select"

    canvas:
        Color: 
            rgba: (0.1529, 0.6157, 0.9608, 0.8) 
